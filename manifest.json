{
  "manifest_version": 2,
  "name": "Noteblock dev",
  "version": "1.2.4",
  "description": "Simple text editor",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "index.html",
    "default_title": "Noteblock dev"
  },
  "permissions": ["storage"]
}
